
{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "api"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "runtime": "nodejs18"
    },
    {
      "source": "ai-microservice",
      "runtime": "nodejs18"
    }
  ]
}
